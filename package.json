{
  "name": "moysklad-url-parser",
  "version": "1.0.0",
  "description": "Разбор с формирование url сервиса МойСклад",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "compile": "tsc",
    "compile:prod": "rm -rf build/ && tsc --build tsconfig.prod.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run lint:fix && npm run compile",
    "build:prod": "rm -rf build/ && npm run lint:fix && npm run compile:prod"
  },
  "keywords": [
    "moysklad",
    "url",
    "parser"
  ],
  "author": "Vitaliy V. Makeev",
  "license": "ISC",
  "devDependencies": {
    "@types/blue-tape": "^0.1.33",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.0.24",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@typescript-eslint/typescript-estree": "^4.5.0",
    "blue-tape": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "lodash": "^4.17.20",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "typescript-eslint-language-service": "^4.1.0"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "ts-replace-all": "^1.0.0"
  }
}
